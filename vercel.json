{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "install": ["npm", "install", "--legacy-peer-deps"],
        "build": ["npm", "run", "build", "--", "--configuration", "production", "--budgets=[]"],
        "outputDirectory": "dist/maestros_angular"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 